<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 
 <!--Cyr zh (Ð¶)-->
 
 <glyph ps-name="afii10072" init-graphics="yes">
  <!--
